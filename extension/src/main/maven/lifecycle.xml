<lifecycles xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/xsd/lifecycle-1.0.0.xsd">
	<lifecycle>
		<id>builder</id>
		<phases>
			<phase>
				<id>scan-references</id>
				<executions>
					<execution>
						<goals>
							<goal>net.runeduniverse.tools.maven.r4m:r4m-maven-extension:scan-references
							</goal>
						</goals>
					</execution>
				</executions>
			</phase>
			<phase>
				<id>pre-preprocessor</id>
			</phase>
			<phase>
				<id>preprocessor</id>
			</phase>
			<phase>
				<id>post-preprocessor</id>
			</phase>
			<phase>
				<id>pre-compiler</id>
			</phase>
			<phase>
				<id>compiler</id>
			</phase>
			<phase>
				<id>post-compiler</id>
			</phase>
			<phase>
				<id>pre-assembler</id>
			</phase>
			<phase>
				<id>assembler</id>
			</phase>
			<phase>
				<id>post-assembler</id>
			</phase>
			<phase>
				<id>pre-linker</id>
			</phase>
			<phase>
				<id>linker</id>
			</phase>
			<phase>
				<id>post-linker</id>
			</phase>
			<phase>
				<id>package-binary</id>
			</phase>
		</phases>
	</lifecycle>
</lifecycles>