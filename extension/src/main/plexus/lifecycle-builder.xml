<component-set>
	<components>
		<component>
			<role>org.apache.maven.lifecycle.Lifecycle</role>
			<implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
			<role-hint>build-invoker</role-hint>
			<configuration>
				<id>build-invoker</id>
				<phases>
					<phase>build</phase>
					<phase>build-test</phase>
				</phases>
			</configuration>
		</component>
		<component>
			<role>org.apache.maven.lifecycle.Lifecycle</role>
			<implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
			<role-hint>builder</role-hint>
			<configuration>
				<id>builder</id>
				<phases>
					<phase>scan-references</phase>
					<phase>pre-preprocessor</phase>
					<phase>preprocessor</phase>
					<phase>post-preprocessor</phase>
					<phase>pre-compiler</phase>
					<phase>compiler</phase>
					<phase>post-compiler</phase>
					<phase>pre-assembler</phase>
					<phase>assembler</phase>
					<phase>post-assembler</phase>
					<phase>pre-linker</phase>
					<phase>linker</phase>
					<phase>post-linker</phase>
					<phase>bin-package</phase>
				</phases>
			</configuration>
		</component>
	</components>
</component-set>