	xsi:schemaLocation="https://api.runeduniverse.net/runes4tools/r4m-pem https://api.runeduniverse.net/runes4tools/r4m-pem-v1.xsd 
	"> -->
<project-execution-model
	xmlns="https://api.runeduniverse.net/runes4tools/r4m-pem"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://api.runeduniverse.net/runes4tools/r4m-pem r4m-pem-v1.xsd">

	<modelVersion>1.0.0</modelVersion>

	<executions>

		<execution id="default" source="override">
			<!-- <triggers> -->
			<!-- <active-profile id="default" /> -->
			<!-- <passive-profile id="test" /> -->
			<!-- </triggers> -->
			<lifecycles>
				<lifecycle id="clean">
				</lifecycle>
				<lifecycle id="default">
					<phases>
						<phase id="compile">
							<goals>
								<goal id="build-invoker-summary">
									<modes>
										<default />
										<dev />
									</modes>
									<groupId>net.runeduniverse.tools.runes4tools</groupId>
									<artifactId>runes4maven-maven-plugin</artifactId>
									<fork>
										<lifecycle id="builder">
											<executions>
												<execution>default</execution>
											</executions>
										</lifecycle>
									</fork>
								</goal>
							</goals>
						</phase>
						<phase id="test-compile">
							<goals>
								<goal id="build-invoker-summary">
									<modes>
										<default />
										<dev />
									</modes>
									<groupId>net.runeduniverse.tools.runes4tools</groupId>
									<artifactId>runes4maven-maven-plugin</artifactId>
									<fork>
										<lifecycle id="builder">
											<executions>
												<execution>test</execution>
											</executions>
										</lifecycle>
									</fork>
								</goal>
							</goals>
						</phase>
					</phases>
				</lifecycle>
				<lifecycle id="site">
				</lifecycle>
			</lifecycles>
		</execution>
	</executions>
</project-execution-model>