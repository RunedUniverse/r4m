<lifecycles xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/xsd/lifecycle-1.0.0.xsd">
	<lifecycle>
		<id>builder</id>
		<phases>
			<phase>
				<id>builder-map-references</id>
				<executions>
					<execution>
						<goals>
							<goal>net.runeduniverse.tools.runes4tools:runes4maven-maven-plugin:builder-map-references
							</goal>
						</goals>
					</execution>
				</executions>
			</phase>
			<phase>
				<id>builder-pre-preprocessor</id>
			</phase>
			<phase>
				<id>builder-preprocessor</id>
			</phase>
			<phase>
				<id>builder-post-preprocessor</id>
			</phase>
			<phase>
				<id>builder-pre-compiler</id>
			</phase>
			<phase>
				<id>builder-compiler</id>
			</phase>
			<phase>
				<id>builder-post-compiler</id>
			</phase>
			<phase>
				<id>builder-pre-assembler</id>
			</phase>
			<phase>
				<id>builder-assembler</id>
			</phase>
			<phase>
				<id>builder-post-assembler</id>
			</phase>
			<phase>
				<id>builder-pre-linker</id>
			</phase>
			<phase>
				<id>builder-linker</id>
			</phase>
			<phase>
				<id>builder-post-linker</id>
			</phase>
			<phase>
				<id>builder-package</id>
			</phase>
		</phases>
	</lifecycle>
</lifecycles>